<?xml version="1.0" encoding="utf-8"?>
<s:Application xmlns:fx="http://ns.adobe.com/mxml/2009"
			   xmlns:s="library://ns.adobe.com/flex/spark"
			   xmlns:mx="library://ns.adobe.com/flex/mx"
			   minWidth="640" minHeight="480"
			   pageTitle="Planner"
			   viewSourceURL="srcview/index.html"
			   applicationComplete="PlannerAPI.initialize()">
	<fx:Declarations>
		<!-- Разместить невизуальные элементы (например, службы или объекты 
		значений) -->
	</fx:Declarations>
	<s:controlBarContent>
		<s:HGroup id="titlePanel"
				  width="163" height="25"
				  horizontalAlign="center" verticalAlign="middle">
			<s:Button id="backButton"
					  label="Назад"
					  click="PlannerAPI.onBackButtonClick()"/>
			<s:Button id="forwardButton"
					  label="Вперёд"
					  click="PlannerAPI.onForwardButtonClick()"/>
		</s:HGroup>
	</s:controlBarContent>
</s:Application>
